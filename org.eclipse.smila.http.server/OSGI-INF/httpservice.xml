<?xml version="1.0" encoding="UTF-8"?>
<component name="HttpService" immediate="true">
    <implementation class="org.eclipse.smila.http.server.internal.HttpServiceImpl" />
    <service>
       <provide interface="org.eclipse.smila.http.server.HttpService"/>
    </service>
    <reference
      name="HttpHandlers"
      interface="org.eclipse.smila.http.server.HttpHandler"
      bind="addHttpHandler"
      unbind="removeHttpHandler"
      cardinality="0..n"
      policy="static" />
    <reference
      name="JettyHandlers"
      interface="org.eclipse.smila.http.server.JettyHandler"
      bind="addJettyHandler"
      unbind="removeJettyHandler"
      cardinality="0..n"
      policy="static" />
</component>
