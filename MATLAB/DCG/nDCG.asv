function nDCG = nDCG(DCG, iDCG)

num_exp = 4;
num_instances = 10;
nDCG = cell(10,4);

% for all instances
for i=1:num_instances
    iDCG_i = iDCG{i};
    % for all experiments
    for j=1:num_exp
        DCG_ij = DCG{i}{j};
        nDCG_ij = DCG_ij ./ iDCG_i;
        nDCG(i,j) = nDCG_ij;
    end
end