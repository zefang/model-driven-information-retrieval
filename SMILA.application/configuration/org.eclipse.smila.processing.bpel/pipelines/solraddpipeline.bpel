<process name="SolrAddPipeline" targetNamespace="http://www.eclipse.org/smila/processor"
  xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:proc="http://www.eclipse.org/smila/processor" xmlns:rec="http://www.eclipse.org/smila/record" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable">

    <import location="processor.wsdl" namespace="http://www.eclipse.org/smila/processor"
    importType="http://schemas.xmlsoap.org/wsdl/" />

  <partnerLinks>
    <partnerLink name="Pipeline" partnerLinkType="proc:ProcessorPartnerLinkType" myRole="service" />
  </partnerLinks>

  <extensions>
    <extension namespace="http://www.eclipse.org/smila/processor" mustUnderstand="no" />
  </extensions>

  <variables>
    <variable name="request" messageType="proc:ProcessorMessage" />
  </variables>

    <sequence>
        
        
        <receive name="start" partnerLink="Pipeline" portType="proc:ProcessorPortType"
      operation="process" variable="request" createInstance="yes" />


        <bpel:flow name="Flow">
            <bpel:scope name="Scope3" isolated="no">
                <bpel:sequence>
                
                		<bpel:assign validate="yes" name="AssignD">
                            <bpel:copy>
                                <bpel:from part="id" variable="request"></bpel:from>
                                <bpel:to part="id" variable="recordsD"></bpel:to>
                            </bpel:copy>
                            <bpel:copy>
                                <bpel:from part="records" variable="request"></bpel:from>
                                <bpel:to part="records" variable="recordsD"></bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        
						<bpel:extensionActivity>
				<proc:invokePipelet name="invokeCreateGraph">
					<proc:pipelet class="it.polimi.mdir.graph.pipelet.CreateGraphPipelet"></proc:pipelet>
					<proc:variables input="recordsD"></proc:variables>
					<proc:configuration>
						<rec:Val key="forceRewrite">false</rec:Val>
					</proc:configuration>
				</proc:invokePipelet>
			</bpel:extensionActivity>
			
			<bpel:extensionActivity>
                <proc:invokePipelet name="invokeSplitCPipelet_ExpD">
       				<proc:pipelet class="it.polimi.mdir.uml.SplitCPipelet" />
       				<proc:variables input="recordsD" />     
   				</proc:invokePipelet>		
            </bpel:extensionActivity>
                        
            <bpel:extensionActivity>
                <proc:invokePipelet name="invokeGraphNavigationPipelet">
                    <proc:pipelet class="it.polimi.mdir.uml.GraphNavigationPipelet"></proc:pipelet>
                    <proc:variables input="recordsD" />
                    <proc:configuration>
              			<rec:Val key="maxHops">2</rec:Val>
              		</proc:configuration>
                </proc:invokePipelet>
            </bpel:extensionActivity>
            
            <extensionActivity>
	        	<proc:invokePipelet name="invokeAnalyzerPayloadSubstitutionPipelet_ExpD">
	            <proc:pipelet class="it.polimi.mdir.uml.AnalyzerPayloadSubstitutionPipelet" />
	            <proc:variables input="recordsD" />
	            <proc:configuration>
	            	<rec:Val key="coreName">text_experiment_D</rec:Val>
	              	<rec:Val key="analysisField">text_D</rec:Val>
	            </proc:configuration>
	          </proc:invokePipelet>
	        </extensionActivity>
                    
			<bpel:extensionActivity>
            	<proc:invokePipelet name="invokeSolrIndexerPipelet_D">
                	<proc:pipelet class="it.polimi.mdir.solr.SolrIndexerPipelet" />
          			<proc:variables input="recordsD" />
          			<proc:configuration>
            			<rec:Val key="executionMode">ADD</rec:Val>
            			<rec:Val key="commitWithin" type="long">10000</rec:Val>
            			<rec:Val key="coreName">text_experiment_D</rec:Val>
	  					<rec:Seq key="smilaFields">
			    			<rec:Val>projectId</rec:Val>
							<rec:Val>projectName</rec:Val>
							<rec:Val>projectName_analyzed</rec:Val>	  	    
							<rec:Val>conceptType</rec:Val>
							<rec:Val>classId</rec:Val>
							<rec:Val>className</rec:Val>
							<rec:Val>className_analyzed</rec:Val>
							<rec:Val>attributeNames</rec:Val>
							<rec:Val>attributeNames_analyzed</rec:Val>
			  			</rec:Seq>
			  			<rec:Seq key="solrFields">
			    			<rec:Val>projectId</rec:Val>
							<rec:Val>projectName</rec:Val>
							<rec:Val>content</rec:Val>
							<rec:Val>conceptType</rec:Val>			
							<rec:Val>classId</rec:Val>
							<rec:Val>className</rec:Val>
							<rec:Val>content</rec:Val>
							<rec:Val>attributeNames</rec:Val>
							<rec:Val>content</rec:Val>
			  			</rec:Seq>
          			</proc:configuration>
				</proc:invokePipelet>
            </bpel:extensionActivity>
                    
                </bpel:sequence><bpel:variables>
                        <bpel:variable name="recordsD" messageType="proc:ProcessorMessage"></bpel:variable>
                    </bpel:variables>
            
                <bpel:sources>
                    <bpel:source linkName="link1"></bpel:source>
                </bpel:sources>
            
            </bpel:scope>
            <bpel:scope name="Scope" isolated="no"><bpel:sequence>
                    
                        <bpel:assign validate="yes" name="AssignC">
                            <bpel:copy>
                                <bpel:from part="id" variable="request"></bpel:from>
                                <bpel:to part="id" variable="recordsC"></bpel:to>
                            </bpel:copy>
                            <bpel:copy>
                                <bpel:from part="records" variable="request"></bpel:from>
                                <bpel:to part="records" variable="recordsC"></bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        
                        <extensionActivity>
                        	<proc:invokePipelet name="invokeSplitCPipelet">
		        				<proc:pipelet class="it.polimi.mdir.uml.SplitCPipelet" />
		        				<proc:variables input="recordsC" />     
		    				</proc:invokePipelet>		
                        </extensionActivity>
                        
                        <extensionActivity>
                			<proc:invokePipelet name="invokeUmlPayloadAdder">
		        				<proc:pipelet class="it.polimi.mdir.uml.UmlPayloadAdderPipelet" />
								<proc:variables input="recordsC" />     
		    				</proc:invokePipelet>
                		</extensionActivity>
                        
				        <extensionActivity>
				          <proc:invokePipelet name="invokeAnalyzerPayloadSubstitutionPipelet">
				            <proc:pipelet class="it.polimi.mdir.uml.AnalyzerPayloadSubstitutionPipelet" />
				            <proc:variables input="recordsC" />
				            <proc:configuration>
				              <rec:Val key="coreName">text_experiment_C</rec:Val>
				              <rec:Val key="analysisField">text_C</rec:Val>
				            </proc:configuration>
				          </proc:invokePipelet>
				        </extensionActivity>
                		
                        <extensionActivity>
                        	<proc:invokePipelet name="invokeSolrIndexerPipelet_C">
                    			<proc:pipelet class="it.polimi.mdir.solr.SolrIndexerPipelet" />
            					<proc:variables input="recordsC" />
            					<proc:configuration>
              						<rec:Val key="executionMode">ADD</rec:Val>
              						<rec:Val key="commitWithin" type="long">10000</rec:Val>
              						<rec:Val key="coreName">text_experiment_C</rec:Val>
			  						<rec:Seq key="smilaFields">
			    						<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>
										<rec:Val>projectName_analyzed</rec:Val>	  	    
										<rec:Val>conceptType</rec:Val>
										<rec:Val>classId</rec:Val>
										<rec:Val>className</rec:Val>
										<rec:Val>className_analyzed</rec:Val>
										<rec:Val>attributeNames</rec:Val>
										<rec:Val>attributeNames_analyzed</rec:Val>
			  						</rec:Seq>
			  						<rec:Seq key="solrFields">
			    						<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>
										<rec:Val>content</rec:Val>
										<rec:Val>conceptType</rec:Val>			
										<rec:Val>classId</rec:Val>
										<rec:Val>className</rec:Val>
										<rec:Val>content</rec:Val>
										<rec:Val>attributeNames</rec:Val>
										<rec:Val>content</rec:Val>
			  						</rec:Seq>
            					</proc:configuration>
                    		</proc:invokePipelet>
                        </extensionActivity>
                    </bpel:sequence>
                    
                    <bpel:variables>
                        <bpel:variable name="recordsC" messageType="proc:ProcessorMessage"></bpel:variable>
                    </bpel:variables>
            
                <bpel:targets>
                    <bpel:target linkName="link1"></bpel:target>
                </bpel:targets>
                <bpel:sources>
                    <bpel:source linkName="link2"></bpel:source>
                </bpel:sources>
            </bpel:scope>
            <bpel:scope name="Scope2" isolated="no"><bpel:sequence>
                    
                        <bpel:assign validate="yes" name="AssignB">
                            <bpel:copy>
                                <bpel:from part="id" variable="request"></bpel:from>
                                <bpel:to part="id" variable="recordsB"></bpel:to>
                            </bpel:copy>
                            <bpel:copy>
                                <bpel:from part="records" variable="request"></bpel:from>
                                <bpel:to part="records" variable="recordsB"></bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        
                        <extensionActivity>
							<proc:invokePipelet name="invokeSplitBPipelet">
								<proc:pipelet class="it.polimi.mdir.uml.SplitBPipelet" />
								<proc:variables input="recordsB" />     
							</proc:invokePipelet>		
						</extensionActivity>
						
                        <extensionActivity>
                        	<proc:invokePipelet name="invokeSolrIndexerPipelet_B">
								<proc:pipelet class="it.polimi.mdir.solr.SolrIndexerPipelet" />
								<proc:variables input="recordsB" />
								<proc:configuration>
									<rec:Val key="executionMode">ADD</rec:Val>
									<rec:Val key="commitWithin" type="long">10000</rec:Val>
									<rec:Val key="coreName">text_experiment_B</rec:Val>
									<rec:Seq key="smilaFields">
										<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>		    
										<rec:Val>className</rec:Val>
										<rec:Val>classId</rec:Val>
										<rec:Val>conceptType</rec:Val>
										<rec:Val>attributeNames</rec:Val>		
									</rec:Seq>
									<rec:Seq key="solrFields">
										<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>
										<rec:Val>className</rec:Val>
										<rec:Val>classId</rec:Val>
										<rec:Val>conceptType</rec:Val>
										<rec:Val>attributeNames</rec:Val>					
									</rec:Seq>
								</proc:configuration>
							</proc:invokePipelet>
                        </extensionActivity>
                    </bpel:sequence>
                    
                    <bpel:variables>
                        <bpel:variable name="recordsB" messageType="proc:ProcessorMessage"></bpel:variable>
                    </bpel:variables>
            
                <bpel:targets>
                    <bpel:target linkName="link2"></bpel:target>
                </bpel:targets>
                <bpel:sources>
                    <bpel:source linkName="link3"></bpel:source>
                </bpel:sources>
            </bpel:scope>
            <bpel:scope name="Scope1" isolated="no"><bpel:sequence>
                    
                        <bpel:assign validate="yes" name="AssignA">
                            <bpel:copy>
                                <bpel:from part="id" variable="request"></bpel:from>
                                <bpel:to part="id" variable="recordsA"></bpel:to>
                            </bpel:copy>
                            <bpel:copy>
                                <bpel:from part="records" variable="request"></bpel:from>
                                <bpel:to part="records" variable="recordsA"></bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        
                        <extensionActivity>
		    				<proc:invokePipelet name="invokeSplitAPipelet">
		        				<proc:pipelet class="it.polimi.mdir.uml.SplitAPipelet" />
		        				<proc:variables input="recordsA" />     
		    				</proc:invokePipelet>
						</extensionActivity>
						
                        <extensionActivity>
	                        <proc:invokePipelet name="invokeSolrIndexerPipelet_A">
            					<proc:pipelet class="it.polimi.mdir.solr.SolrIndexerPipelet" />
            					<proc:variables input="recordsA" />
            					<proc:configuration>
	              					<rec:Val key="executionMode">ADD</rec:Val>
              						<rec:Val key="commitWithin" type="long">10000</rec:Val>
              						<rec:Val key="coreName">text_experiment_A</rec:Val>
			  						<rec:Seq key="smilaFields">
				    					<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>		    
										<rec:Val>content</rec:Val>	
			  						</rec:Seq>
			  						<rec:Seq key="solrFields">
				    					<rec:Val>projectId</rec:Val>
										<rec:Val>projectName</rec:Val>
										<rec:Val>content</rec:Val>				
			  						</rec:Seq>
            					</proc:configuration>
          					</proc:invokePipelet>
                        </extensionActivity>
                    </bpel:sequence>
                    
                    <bpel:variables>
                        <bpel:variable name="recordsA" messageType="proc:ProcessorMessage"></bpel:variable>
                    </bpel:variables>
            
                <bpel:targets>
                    <bpel:target linkName="link3"></bpel:target>
                </bpel:targets>
            </bpel:scope>
            <bpel:links>
                <bpel:link name="link1"></bpel:link>
                <bpel:link name="link2"></bpel:link>
                <bpel:link name="link3"></bpel:link>
            </bpel:links>
        </bpel:flow>
 
<!--

SOLR-ANALYZER-PIPELET

		<extensionActivity>
          <proc:invokePipelet name="invokeSolrAnalyzerPipelet">
            <proc:pipelet class="it.polimi.mdir.solr.SolrAnalyzerPipelet" />
            <proc:variables input="request" />
            <proc:configuration>
			  <rec:Val key="writerType">xml</rec:Val>
			  <rec:Val key="indent">true</rec:Val>
              <rec:Val key="coreName">test_core</rec:Val>
			  <rec:Seq key="smilaFields">
			    <rec:Val>projectId</rec:Val>
				<rec:Val>content</rec:Val>
			  </rec:Seq>
			  <rec:Seq key="solrAnalysisTypes">
			    <rec:Val>projectId</rec:Val>
				<rec:Val>text_A</rec:Val>
			  </rec:Seq>
            </proc:configuration>
          </proc:invokePipelet>
        </extensionActivity>
-->
        
        <reply name="end" partnerLink="Pipeline" portType="proc:ProcessorPortType" operation="process" variable="request" />
        <exit />
    </sequence>
</process>
