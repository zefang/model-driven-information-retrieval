<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:proc="http://www.eclipse.org/smila/processor">

  <process name="proc:AddPipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:AddPipeline" port="ProcessorPort" />
    </provide>    
  </process>

  <process name="proc:DeletePipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:DeletePipeline" port="ProcessorPort" />
    </provide>    
  </process>
  
   <process name="proc:SearchPipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:SearchPipeline" port="ProcessorPort" />
    </provide>    
  </process>
  
   <process name="proc:AddFeedPipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:AddFeedPipeline" port="ProcessorPort" />
    </provide>    
  </process>
  
  <process name="proc:XmlDumpSplitterPipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:XmlDumpSplitterPipeline" port="ProcessorPort" />
    </provide>    
  </process>
  
  <process name="proc:XmlDumpAddPipeline">
    <in-memory>true</in-memory>
    <provide partnerLink="Pipeline">
      <service name="proc:XmlDumpAddPipeline" port="ProcessorPort" />
    </provide>    
  </process>  
</deploy>
